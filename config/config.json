{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "jq-web-part-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/jqWebPart/JQWebPartWebPart.js",
          "manifest": "./src/webparts/jqWebPart/JQWebPartWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": "node_modules/jquery/dist/jquery.min.js",
