"bootstrap": {
"path": "node_modules/bootstrap/dist/js/bootstrap.min.js",
"globalName": "bootstrap"
}
},
  
  "localizedResources": {
    "JQWebPartWebPartStrings": "lib/webparts/jqWebPart/loc/{locale}.js"
  }
}
